{
  "name": "@thequinndev/route-manager",
  "version": "0.0.1-alpha.3",
  "description": "",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "vitest --run",
    "test:w": "vitest",
    "coverage": "vitest --coverage",
    "build": "npx rimraf dist && pnpm build:esm && pnpm build:cjs",
    "build:esm": "rollup -c ./rollup.config.js",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "lint": "npx eslint",
    "lint-ts": "tsc --noEmit",
    "prepare": "husky"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thequinndev/route-manager.git"
  },
  "author": "thequinndev",
  "license": "MIT",
  "dependencies": {
    "openapi3-ts": "^4.5.0",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "keywords": [
    "openapi3",
    "ts",
    "schema",
    "typescript",
    "openapi",
    "endpoints",
    "zod",
    "routes"
  ],
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.18.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.34.0",
    "express": "^4.21.2",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.48.1",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "src/*.ts": "prettier --write --ignore-unknown"
  }
}
